                          j:hiddenTemplate="true"
                          j:icon="/modules/siteSettings/templates/files/users_into.png"
                          j:priority="100"
                          j:requiredPermissionNames="siteAdminSiteRoles"
                          j:titleKey="siteSettings.manageSiteRoles"
                          jcr:mixinTypes="jmix:rbTitle"
                          jcr:primaryType="jnt:contentTemplate">
                <pagecontent jcr:primaryType="jnt:contentList">
                    <manage-site-roles jcr:primaryType="jnt:manageRoles" roleGroup="site-role"/>
                </pagecontent>
            </manageSiteRoles>
