                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <site-settings-base j:view="siteSettings"
                              jcr:mixinTypes="jmix:useUILocale"
                              jcr:primaryType="jnt:template">
                        <manageLanguages j:applyOn="jnt:virtualsite"
                                j:defaultTemplate="false"
                                j:hiddenTemplate="true"
                                j:icon="/modules/siteSettings/templates/files/signpost.png"
                                j:priority="100"
                               j:requiredPermissionNames="siteAdminLanguages"
                                j:titleKey="siteSettings.label.manageLanguages"
                                jcr:mixinTypes="jmix:rbTitle"
                               jcr:primaryType="jnt:contentTemplate">
                  <pagecontent jcr:primaryType="jnt:contentList">
                      <manage-languages jcr:primaryType="jnt:siteSettingsManageLanguages"/>
                  </pagecontent>
              </manageLanguages>
                        <wcagCompliance j:applyOn="jnt:virtualsite"
                               j:defaultTemplate="false"
                               j:hiddenTemplate="true"
                              j:icon="/modules/siteSettings/templates/files/text_rich_colored.png"
                              j:priority="100"
                              j:requiredPermissionNames="siteAdminWcagCompliance"
                              j:titleKey="siteSettings.label.wcagCompliance"
                               jcr:mixinTypes="jmix:rbTitle"
                               jcr:primaryType="jnt:contentTemplate">
                  <pagecontent jcr:primaryType="jnt:contentList">
                      <wcag-compliance jcr:primaryType="jnt:siteSettingsWcagCompliance"/>
                  </pagecontent>
              </wcagCompliance>
                        <htmlFiltering j:applyOn="jnt:virtualsite"
                              j:defaultTemplate="false"
                              j:hiddenTemplate="true"
                             j:icon="/modules/siteSettings/templates/files/text_rich_colored.png"
                             j:priority="100"
                             j:requiredPermissionNames="siteAdminHtmlSettings"
                             j:titleKey="siteSettings.label.htmlFiltering"
                              jcr:mixinTypes="jmix:rbTitle"
                              jcr:primaryType="jnt:contentTemplate">
                  <pagecontent jcr:primaryType="jnt:contentList">
                      <html-filtering jcr:primaryType="jnt:siteSettingsHtmlFiltering"/>
                  </pagecontent>
              </htmlFiltering>
                        <vanityURLSetting j:applyOn="jnt:virtualsite"
                                 j:defaultTemplate="false"
                                 j:hiddenTemplate="true"
                                 j:icon="/modules/siteSettings/templates/files/urlmapping.png"
                                 j:priority="100"
                                j:requiredPermissionNames="siteAdminUrlmapping"
                                 j:titleKey="siteSettings.label.vanityURLSettings"
                                 jcr:mixinTypes="jmix:rbTitle"
                                jcr:primaryType="jnt:contentTemplate">
                  <pagecontent jcr:primaryType="jnt:contentList">
                                <pager displayNbOfItemsPerPage="true"
                            displayNbOfItemsPerPageValues="5,10,25,50,100"
                             displayPager="true"
                             j:bindedComponent="/modules/siteSettings/templates/site-settings-base/vanityURLSetting/pagecontent/vanityurlslist"
                            jcr:mixinTypes="jmix:accessControlled"
                            jcr:primaryType="jnt:pager"
                            nbOfPages="10"
                             pageSize="10">
                          <j:acl jcr:primaryType="jnt:acl"/>
                      </pager>
                      <vanityurlslist jcr:mixinTypes="jmix:accessControlled"
                                      jcr:primaryType="jnt:siteSettingsVanityUrlsList">
                          <j:acl jcr:primaryType="jnt:acl"/>
                      </vanityurlslist>
                                <pager-1 displayNbOfItemsPerPage="true"
                              displayNbOfItemsPerPageValues="5,10,25,50,100"
                               displayPager="true"
                               j:bindedComponent="/modules/siteSettings/templates/site-settings-base/vanityURLSetting/pagecontent/vanityurlslist"
                              jcr:mixinTypes="jmix:accessControlled"
                              jcr:primaryType="jnt:pager"
                              nbOfPages="10"
                               pageSize="10">
                          <j:acl jcr:primaryType="jnt:acl"/>
                      </pager-1>
                  </pagecontent>
              </vanityURLSetting>
                        <manageModules j:applyOn="jnt:virtualsite"
                              j:defaultTemplate="false"
                              j:hiddenTemplate="true"
                             j:icon="/modules/siteSettings/templates/files/briefcase_document.png"
                             j:priority="100"
                             j:requiredPermissionNames="siteAdminTemplates"
                             j:titleKey="siteSettings.label.manageModules"
                              jcr:mixinTypes="jmix:rbTitle"
                              jcr:primaryType="jnt:contentTemplate">
                  <pagecontent jcr:primaryType="jnt:contentList">
                      <manage-modules jcr:primaryType="jnt:siteSettingsManageModules"/>
                  </pagecontent>
              </manageModules>
                        <manageGroups j:applyOn="jnt:virtualsite"
                             j:defaultTemplate="false"
                             j:hiddenTemplate="true"
                             j:icon="/modules/siteSettings/templates/files/users_into.png"
                             j:priority="100"
                             j:requiredPermissionNames="siteAdminGroups"
                             j:titleKey="siteSettings.label.manageGroups"
                             jcr:mixinTypes="jmix:rbTitle"
                             jcr:primaryType="jnt:contentTemplate">
